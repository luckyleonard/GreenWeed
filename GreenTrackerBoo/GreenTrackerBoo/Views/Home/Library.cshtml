
@{
    ViewBag.Title = "Library";
    Layout = "~/Views/Shared/_LayoutOther.cshtml";
}

<h2></h2>

<section class="container" id="plantTable" >
    <div class="container" id="wrapper">
        <center>
            <h2>Invasive Plant Library </h2>
            <br />
            <table id="keywords" class="table table-hover">
                <thead>
                    <tr id="title" style="background-color: #b5eaaa;">
