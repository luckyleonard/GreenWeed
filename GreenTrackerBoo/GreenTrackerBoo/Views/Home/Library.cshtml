                    <td><button style="margin-top:0%" type="button" class="btn yellow-btn normal-btn btn-aaa" id="th">Report</button></td>
                </tr>
                <tr id="plant14" onclick="window.document.location = 'https://greentracker.azurewebsites.net/Plants/WaterHyacinth';">
                    <td class="lalign"><img class="profile-img" src="~/css/images/plants/profile/wh-profile.jpg" /></td>
