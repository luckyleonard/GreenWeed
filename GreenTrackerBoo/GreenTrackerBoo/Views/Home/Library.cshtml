<!-- ***This is the Library page of the whole website, including all the plants
     basic infor and the links to individual pages.***-->

@{
    ViewBag.Title = "Library";
    Layout = "~/Views/Shared/_LayoutOther.cshtml";
}

<style>
    .profile-img {
        width: 85%;
        margin-left: -17%;
    } 


</style>

<section class="container" id="plantTable" >
    <div class="container" id="wrapper" style="text-align:center">
            <h2>Invasive Plant Library </h2>
            <br />
        <table id="librarytable" class="table table-hover display">
            <thead>
                <tr id="title" style="background-color: #b5eaaa;">
                    <!-- ***Title of table***-->
                    <th><span>Picture</span></th>
                    <th><span>Common Name</span></th>
                    <th><span>Scientific Name</span></th>
                    <th><span>Report</span></th>
                </tr>
            </thead>
            <tbody>
                <!-- ***Content of table***-->
                <tr id="plant1" onclick="window.document.location = 'https://greentracker.azurewebsites.net/Plants/AlligatorWeed';">
                    <td class="lalign"><img class="profile-img" src="~/css/images/plants/profile/alligator-profile.jpg" /></td>
                    <td>Alligator Weed</td>
                    <td>Alternanthera philoxeroides</td>
                    <td><button style="margin-top:0%" type="button" class="btn yellow-btn normal-btn btn-aaa" id="aw">Report</button></td>
                </tr>
                <tr id="plant2" onclick="window.document.location = 'https://greentracker.azurewebsites.net/Plants/BlackKnapweed';">
                    <td class="lalign"><img class="profile-img" src="~/css/images/plants/profile/black-knapweed-profile.jpg" /></td>
                    <td>Black Knapweed</td>
                    <td>Centaurea nigra L.</td>
